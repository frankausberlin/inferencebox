# Environment variables for dsbase
NODE_ENV=development
PORT=3000
DATABASE_URL=postgresql://user:password@localhost:5432/dsbase
SECRET_KEY=your-secret-key-here

# Jupyter environment variables
JUPYTER_TOKEN=test-token
JUPYTER_PASSWORD=test-password

# vLLM environment variables (auto-configured by configure.sh)
VLLM_MODEL=microsoft/DialoGPT-medium
VLLM_GPU_MEMORY_UTILIZATION=0.8
VLLM_MAX_MODEL_LEN=4096
VLLM_HOST=0.0.0.0
VLLM_PORT=8000
VLLM_TENSOR_PARALLEL_SIZE=1
VLLM_QUANTIZATION=awq
VLLM_DISABLE_FRONTEND_MULTIPROCESSING=true

# Open WebUI environment variables
OPENAI_API_BASE_URL=http://vllm-server:8000/v1
OPENAI_API_KEY=sk-test-key-for-cpu-fallback
WEBUI_SECRET_KEY=your-webui-secret-key-here
WEBUI_HOST=0.0.0.0
WEBUI_PORT=3000

# GPU configuration
NVIDIA_VISIBLE_DEVICES=all
NVIDIA_DRIVER_CAPABILITIES=compute,utility
VLLM_MODEL_NAME=microsoft/DialoGPT-medium
GPU_TYPE=NVIDIA
GPU_COUNT=1
TOTAL_VRAM_MB=8192
