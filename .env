# Environment variables for inferencebox
NODE_ENV=development
PORT=3000
DATABASE_URL=postgresql://user:password@localhost:5432/inferencebox
SECRET_KEY=your-secret-key-here

# Jupyter environment variables
JUPYTER_TOKEN=test-token
JUPYTER_PASSWORD=test-password

# Open WebUI environment variables
OPENAI_API_BASE_URL=http://ollama:11435/v1
OPENAI_API_KEY=sk-test-key-for-cpu-fallback
WEBUI_SECRET_KEY=your-webui-secret-key-here
WEBUI_HOST=0.0.0.0
WEBUI_PORT=8080
DATABASE_URL=sqlite:///./data/webui.db

# Ollama environment variables
OLLAMA_HOST=0.0.0.0:11435
OLLAMA_MODELS_DIR=/root/.ollama/models
OLLAMA_KEEP_ALIVE=24h
OLLAMA_MAX_LOADED_MODELS=1
OLLAMA_NUM_PARALLEL=1
OLLAMA_NUM_THREAD=4
OLLAMA_RUNNERS_DIR=/tmp/runners
OLLAMA_TMPDIR=/tmp

# GPU configuration
NVIDIA_VISIBLE_DEVICES=all
NVIDIA_DRIVER_CAPABILITIES=compute,utility
GPU_TYPE=NVIDIA
GPU_COUNT=1
TOTAL_VRAM_MB=8192
