# Environment variables for inferencebox
NODE_ENV=development
PORT=3000
DATABASE_URL=postgresql://user:password@localhost:5432/inferencebox
SECRET_KEY=your-secret-key-here

# Jupyter environment variables
JUPYTER_TOKEN=your-jupyter-token-here
JUPYTER_PASSWORD=your-jupyter-password-here

# Open WebUI environment variables
OPENAI_API_BASE_URL=http://localhost:11435/v1
OPENAI_API_KEY=your-openai-api-key-here
WEBUI_SECRET_KEY=your-webui-secret-key-here
WEBUI_HOST=0.0.0.0
WEBUI_PORT=3000
OLLAMA_BASE_URL=http://datascience-env:11435
ENABLE_OLLAMA_API=true

# Ollama environment variables
OLLAMA_HOST=0.0.0.0:11435
OLLAMA_GPU_LAYERS=35
OLLAMA_MAX_LOADED_MODELS=1
OLLAMA_MAX_QUEUE=512
OLLAMA_RUNNERS_DIR=/tmp/runners
OLLAMA_TMPDIR=/tmp

# GPU configuration
NVIDIA_VISIBLE_DEVICES=all
NVIDIA_DRIVER_CAPABILITIES=compute,utility